0340*
0350WRITE LINE (AL=79)
0360   // A(*) / B(*)
0370IF *UBOUND(C,2) > 1
0380THEN
0390  WRITE 001T 'C' 0012T 'N'
0400  FOR I 1 10
0410    FOR J 1 5
0420      WRITE C(I,1,J) C(I,2,J)
0430    END-FOR
0440  END-FOR
