0010** Example 'DPIEX1': DEFINE PRINTER
0020************************************************************************
0030*
0040SET CONTROL 'XI+'        /* SWITCH INFOLINE MODE ON
0050SET CONTROL 'XT'         /* INFOLINE TOP
0060*
0070DEFINE PRINTER (1) OUTPUT 'INFOLINE'
0080WRITE (1) 'EXECUTING' *PROGRAM 'BY' *INIT-USER
0090WRITE 'TEST OUTPUT'
0100EJECT                    /* FORCE PHYSICAL I/O
0110*
0120SET CONTROL 'X'          /* SWITCH BACK TO NORMAL
0130*
0140END
